	String inputAge = request.getParameter("Age");
	int valAge = Integer.parseInt(inputAge);
	
	if(valAge>=20) {
		out.print("당신의 나이는 " + valAge + "살 이므로 주류 구매가 불가능합니다." +"<br>");
		out.print("처음으로 이동");
	}
		
	
