<body>
	
	
	<%

	int age1, age2;
	age1= Integer.parseInt(request.getParameter("age"));
	age2= Integer.parseInt(request.getParameter("age"));
	
	if(age1>=20) {
		response.sendRedirect("responseAge_03.jsp?age=" +age1);
	}else if(age2<20){
		response.sendRedirect("responseAge_03.jsp?age=" +age2);
	}
	
	%>
	
	
	
	
</body>
