    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<titl>나이 판단</title>
</head>
<body>
	
	
	<%
	
	//성인인지 아닌지, 나이, 가능 불가능 넘기기
	
	int age;
	age= Integer.parseInt(request.getParameter("age"));
	string adult, possible;
	
	if(age>=20){
		response.sendRedirect("responseAge_07.jsp?age=" +age);
		response.sendRedirect("responseAge_07.jsp?adult=" +adult);
		response.sendRedirect("responseAge_07.jsp?possible=" +possible);
	}else{
		response.sendRedirect("responseAge_07.jsp?age=" +age);
		response.sendRedirect("responseAge_07.jsp?adult=" +adult);
		response.sendRedirect("responseAge_07.jsp?possible=" +possible);
		
	}
	
	%>
	
	
	
	
</body>
</html>